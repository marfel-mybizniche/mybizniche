
body.intro-active {
    overflow: hidden;
    margin-right: 17px;
}

.home {
    .grid-container {
        max-width: 75vw;
    
        @media(max-width:1599px) {
            max-width: 1200px;
        }

        @media(max-width:1023px) {
            max-width: none;
        }
    }

    @media(max-width:1440px) {
        #header {
            .hsnav-s7 {
                background: rgba($color-white, 0.8);
            }
        }  
        .dark-bg {
            #header {
                .hsnav-s7 {
                    background: rgba($color-black, 0.3);
                }
            }
                    
        }
    }
}

#content {
    width: 100%;
    position: relative;
    overflow: hidden;
}

.navlogo {
    span {
        position: relative;
        display: inline-block;
    }
}

#intro {
    position: absolute;
    left:0;
    right: 0;
    top:0;
    bottom: 0;
    background-color: $color-white;
    z-index: 998; 
    overflow: hidden;

    .in-1 {
        position: absolute;
        left: 0;
        right: 0;
        top:0;
        height: 100%;
        background: url(../img/bg-intro.jpg) no-repeat center / cover;
    }

    .in-2 {
        opacity: 0;
        .in-logo {
            position: absolute;
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
            margin:-80px 0 0 78px;
            opacity: 0;
        }
        .in-info {
            position: absolute;
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
            font-size: 16px;
            letter-spacing: 4px;
            color: $color-dark;
            font-weight: bold;
            z-index: 1;
            white-space: nowrap;

            b {
                position: relative;
                display: inline-block;
            }

            em, i {
                font-weight: normal;
                color: $color-grey;
            }
        }
    }

    .in-3 {
        position: absolute;
        white-space: nowrap;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        font-size: 16px;
        letter-spacing: 4px;
        color: $color-dark;
        font-weight: bold;
        opacity: 0;

        em, i {
            display: inline-block;
            font-weight: normal;
            color: $color-grey;
        }
    }

    .in-4 {
        position: absolute;
        left: 50%;
        top:calc(50% - 60px);
        transform: translate(-50%, -50%);
        opacity: 0;
    }

    .in-5 {
        position: absolute;
        left: 50%;
        top:calc(50% - 90px);
        transform: translate(-50%, -50%);
        width: 60px;
        text-align: center;
        opacity: 0;
        
        .in-5a {
            margin: -15px 0 10px;
            position: relative;
            top: 0;
            opacity: 1;
        }
        .in-5b {
            position: relative;  
            top: 0;   
            opacity: 1;       
        }
    }
    
    .in-6 {
        position: absolute;
        left: 50%;
        top:calc(50% - 80px);
        transform: translate(-50%, -50%);
        width: 60px;
        text-align: center;
        opacity: 0;
        margin-top: 30px;
        
        .in-6a {
            margin: 0;
        }
        .in-6b {
            position: absolute;
            left: -12px;
            top: -12px;
        }
        .in-6c {
            position: absolute;
            top: 10px;
            right: -10px;
        }
        .in-6d {
            position: absolute;
            top: 35px;
            left: -10px;
        }
    }

    .in-7 {
        position: absolute;
        left: calc(50% + 5px);
        top:calc(50% - 78px);
        transform: translate(-50%, -50%);
        width: 75px;
        text-align: center;
        opacity: 0;
        
        .in-7a {
            margin: 0;
        }
        .in-7b {
            position: absolute;
            left: 16px;
            top: 20px;
        }
        .in-7c {
            position: absolute;
            top: 18px;
            right: 10px;
            z-index: -1;
        }
    }

    .in-8 {
        position: absolute;
        left: calc(50% + 5px);
        top:calc(50% - 78px);
        transform: translate(-50%, -50%);
        width: 75px;
        text-align: center;
        
        .in-8a {
            margin: 0;
            opacity: 0;
        }
        .in-8b {
            position: absolute;
            left: 16px;
            top: 20px;
            opacity: 0;
        }
    }

    .skip-intro {
        position: absolute;
        right: 30px;
        bottom: 30px;
        font-size: 14px;
        text-transform: uppercase;
        color: $color-dark;
        transform: rotate(90deg);
        transform-origin: top;
        min-height: 75px;
    
        a {
            position: absolute;
            white-space: nowrap;
            top: 0;
            color: $color-dark;

            &:hover {
                color: $color-primary;
            }
        }
    }

    @media(max-width: 1024px) {
        //display: none;
    }
}

.sec-start {
    position: relative;
    //z-index: 5;

    .first-trigger {
        position: relative;
        width: 100%;
        background: red;;
    }
    
    .second-trigger {
        position: relative;
        width: 100%;
        background: blue;
    }

    .start-trigger {
        position: absolute;
        top:10px;
        left: 0;
        right: 0;
        z-index: -1;

        .st-1 {
            position: relative;
            min-height: 100vh;

        }
        .st-2 {
            position: relative;
            min-height: 100vh;
        }
        .st-3 {
            position: relative;
            min-height: 100vh;
        }
    }


    .start-side {
        position: fixed;
        top: 50%;
        transform: translate(0, -45%);
        opacity: 0;
        z-index: 99;

        //top:20%; with h6
        top:21%;
        transform: none;
        
        .ss-h6 {
            font-weight: bold;
            font-size: 16px;
            letter-spacing: 4px;
            color: $color-dark;
            position: relative;

            b,strong {
                color: $color-primary;
            }

            i,em {
                font-weight: normal;
                color: $color-grey;
            } 
        }

        ul.text { 
            font-size: 140px;
            font-size: 7vw;
            margin: 0;
            padding:0;
            position: relative;

            li {
                list-style: none;
                font: bold 140px/1 $heading-font-family;
                font-size: 7vw;
                margin: 0;
                padding: 0;
                color: $color-primary;
                position: relative;

                span {
                    color: $color-dark;
                }
            } 
        }

        ul.link {
            margin:0 0 0 18px;
            padding:0;
            position: relative;
            opacity: 0;
            
            li {
                list-style: none;
                margin:20px 0;
                padding:0;
                a {
                    position: relative;
                   // font-size: 15px;
                    font-size: .75vw;
                    font-weight: bold;
                    letter-spacing: 1.5px;
                    color: $color-dark;
                    text-transform: uppercase;
                    display: inline-block;
                    position: relative;

                    &:before {
                        content: "";
                        position: absolute;
                        top: 16px;
                        left: -45px;
                        width: 40px;
                        border-top: 1px solid $color-primary;
                        transition: all 0.3s ease;
                    }
                    &:after {
                        content: "+";
                        font-weight: 200;
                        font-size: 20px;
                        margin: -5px 0 0 15px;
                        color: $color-primary;
                        transition: all 0.3s ease;
                    }

                    &:hover {
                        &:before {
                            left: -30px;
                            width: 25px;
                        }
                        &:after {
                            content: "\276F";
                            font-size: 14px;
                            margin-top: 0;
                        }
                    }
                }
            }
        }

        @at-root .sec-start.si .start-side {
            opacity: 1;
            visibility: visible;
        }

        @at-root .sec-start.sm .start-side {
            opacity: 1;
            visibility: visible;
        }

        
        @at-root .sec-start.sr .start-side {
            opacity: 1;
            visibility: visible;
            top: 30%;
            
            ul.text {
                li {
                    //font-size: 40px;
                    font-size: 2.5vw;
                }
            }
        }

        @media(max-width: 1600px) {
            ul.text {
                li {
                    //font-size: 90px;
                }
            }
        }

        @media(max-width: 1023px) {
            left: 30px;
            right: 30px;
        }

        @media(max-width: 767px) {
            opacity: 0;
            
            .ss-h6 {
                font-size: 11px;
            }

            ul.text {
                opacity: 1;
                li {
                    font-size: 48px;
                }
            }

            ul.link {
                opacity: 1;

                li {
                    margin: 15px 0;

                    a {
                        font-size: 12px;

                        &:before {
                            top: 18px;
                            left: -20px;
                            width: 15px;
                        }

                        &:after {
                            margin-left: 10px;
                        }

                    }
                }
            }
        }

        @media(max-width: 413px) {
            left: 15px;
            right: 15px;

            ul.text {
                li {
                    font-size: 40px;
                }
            }


        }

    }



