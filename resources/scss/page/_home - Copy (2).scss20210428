.scroll {
    position: fixed;
    z-index: 999;
    right: 40px;
    bottom: 0;
    pointer-events: none;

    .line {
        display: block;
        height: 180px;
        position: relative;

        &:before, &:after {
            content: "";
            position: absolute;
            top:0;
            left: 50%;
            width: 1px;
            background: $color-primary;
            height: 100%;
            margin: 0 0 0 -1px;
        }
        &:after {
            background: $color-dark;
            height: 60px;
        }
        
        .circle {
            position: relative;
            top: -40px;
            display: block;
            border-radius: 50%;
            width: 30px;
            height: 30px;
            border-radius: 50%;
            border: solid 2px rgba($color-dark, 0.3);

            &:before {
                content: "";
                width: 6px;
                height: 6px;
                border-radius: 50%;
                background: $color-primary;
                position: absolute;
                left: 50%;
                top: 50%;
                margin: -3px 0 0 -3px;
            }

            .big {
                display: block;
                position: absolute;
                border: solid 2px rgba($color-dark, 0.2);
                border-radius: 50%;
                width: 600px;
                height: 600px;
                left: 50%;
                top: 50%;
                transform: translate(-50%, -50%);
                //z-index: -1;
            }
        }
        
    }


    .text {
        position: absolute;
        color: $color-dark;
        transform: rotate(90deg);
        bottom: 50px;
        text-transform: uppercase;
        line-height: 1;
        right: -40px;
        font-size: 14px;
    }

    
    @media(max-width: 1023px) {
        .line {
            .circle {
                .big {
                    display: none;
                }
            }
        }
        .text {
            display: none;
        }
    }
    
    @media(max-width: 767px) {
        right: 30px;

        .line {
            &:after {
                height: 35px;
            }
        }
    }
}

