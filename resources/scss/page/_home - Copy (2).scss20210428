            width: calc(100% - 20px);
            height: 30%;
            position: relative;
            margin: 0 auto;

            &:before {
                content: "";
                position: absolute;
                background: $color-primary;
                top:0;
                bottom: 0;
                left: 0;
                right: 0;
                z-index: -1;
                transform: skewY(-5deg);
            }

            &.rb2 {
                height: 38%;
            }

            &.rb3 {
                height: 46%;
            }
        }

        .roi-bot {
            position: relative;
            text-align: right;
            
            &:before {
                content: "";
                position: absolute;
                left: 50%;
                background: #27272f;
                height: 200px;
                margin-left: -50vw;
                width: 100vw;
                transform: skewY(-3deg);
                bottom: -150px;
                transform-origin: top;
            }
        }
        .roi-hat {
            position: absolute;
            display: inline-block;
            width: 40%;
            max-width: 100%;
            text-align: center;
            right: 1%;
            bottom: -115px;
            transform: rotate(-5deg);

            .rh1 {
                margin-bottom: -25px;
                position: relative;
                transform: rotate(3deg);
            }
        }

    }
    
}


